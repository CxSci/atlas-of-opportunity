    {
      "title": "Business Counts",
      "metrics": [
        {
          "key": "business_counts",
          "type": "stacked_area",
          "x": {
            "key": "year"
          },
          "y": {
            "key": "value",
            "format": "number"
          },
          "z": {
            "key": "anzsic"
          }
        },
        {
          "key": "business_counts",
          "type": "text",
          "x": {
            "key": "anzsic",
            "options": {
              "style": "square"
            }
          },
          "y": {
            "key": "value",
            "format": "number"
          },
          "filters": [
            {
              "key": "year",
              "control": "select",
              "title": "Year",
              "default_value": "2019"
            }
          ]
        }
      ]
