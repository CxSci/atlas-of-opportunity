            "numberFormat": {
              "style": "currency",
              "currency": "AUD"
            }
          },
          "filters": [
            {
              "key": "type",
              "values": ["Apartments"]
            }
          ]
        },
        {
          "title": "Houses: Median Weekly Rent",
          "key": "residential_housing_median",
          "type": "text",
          "x": {
            "key": "rooms"
          },
          "y": {
            "key": "rent",
            "format": "number",
            "numberFormat": {
              "style": "currency",
              "currency": "AUD"
            }
          },
          "filters": [
            {
              "key": "type",
              "values": ["Houses"]
            }
          ]
        }
