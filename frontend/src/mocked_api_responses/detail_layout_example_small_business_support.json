        {
          "title": "Population",
          "key": "population",
          "type": "text",
          "x": {
            "key": "gender"
          },
          "y": {
            "key": "count",
            "format": "number"
          }
        },
        {
          "title": "Fraction of South Australian Population Living in this Region",
          "key": "popfraction",
          "type": "text",
          "format": "number",
          "numberFormat": {
            "style": "percent",
            "maximumFractionDigits": 20
          }
        },
        {
          "title": "Projected Population",
          "key": "pop_proj",
